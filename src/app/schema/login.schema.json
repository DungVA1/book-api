{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "$id": "http://blog-js.com/login.schema",
  "required": [
    "userName",
    "password"
  ],
  "properties": {
    "userName": {
      "description": "The account or email of user",
      "type": "string",
      "minLength": 1,
      "maxLength": 255
    },
    "password": {
      "description": "The password of user",
      "type": "string",
      "minLength": 1,
      "maxLength": 32
    }
  }
}